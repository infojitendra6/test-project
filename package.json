{
  "name": "examadda.api.service",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "cross-env NODE_ENV=test PORT=8080 MONGO_URL=mongodb://localhost:27017/mern-test node_modules/.bin/nyc node --harmony-proxies node_modules/.bin/ava",
    "test:api": "mocha --reporter list api-tests",
    "test-watch:api": "mocha --watch api-tests",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "start:examaddaworker": "cross-env NODE_ENV=staging nodemon examadda-worker.js",
    "build:examaddaworker": "cross-env NODE_ENV=production webpack --config webpack.autoworker.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon examadda-api.js",
    "start:worker": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon examadda-worker.js",
    "start:prod": "cross-env NODE_ENV=production node examadda-api.js"
  },
